<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>net45;netstandard15</TargetFrameworks>
		<DefineConstants></DefineConstants>
	</PropertyGroup>
	<PropertyGroup>
		<Version>1.0.1.0</Version>
		<Description>Asynchronous STOMP 1.2 client library for .NET.</Description>
		<Company>Carlos Campo</Company>
		<Copyright>Copyright © Carlos Campo 2020</Copyright>
	</PropertyGroup>
	<PropertyGroup>
		<Authors>Carlos Campo</Authors>
		<PackageTags>stomp;nms;activemq;message broker client</PackageTags>
		<PackageProjectUrl>https://github.com/krlito/StompNet</PackageProjectUrl>
		<RepositoryUrl>https://github.com/krlito/StompNet</RepositoryUrl>
		<PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
		<IncludeSymbols>true</IncludeSymbols>
	</PropertyGroup>
	<ItemGroup>
		<None Include="..\\LICENSE.txt" Pack="true" PackagePath="$(PackageLicenseFile)"/>
	</ItemGroup>
</Project>